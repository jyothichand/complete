pipeline{
	agent {label 'tester'}
	stages{
stage ("Build"){

			steps { 
				echo 'building the application..'
			      }
			}
stage ("test"){

			steps { 
				echo 'testing the application..'
			      }
			}
		
stage ("Deploy"){
			steps { 
				echo 'Here deploying  the application..'
			      }
			}
		
		}
}
